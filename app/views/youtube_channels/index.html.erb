<p id="notice"><%= notice %></p>

<h1>Youtube Channels</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @youtube_channels.each do |youtube_channel| %>
      <tr>
        <td><%= youtube_channel.title %></td>
        <td><%= youtube_channel.description %></td>
        <td><%= youtube_channel.owner %></td>
        <td><%= link_to 'Show', youtube_channel %></td>
        <td><%= link_to 'Edit', edit_youtube_channel_path(youtube_channel) %></td>
        <td><%= link_to 'Destroy', youtube_channel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Youtube Channel', new_youtube_channel_path %>
